{
  "manifest_version": 2,

  "name": "Are you sure?",
  "description": "Confirm before you visit certain websites",
  "version": "2.1",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "permissions": [
    "<all_urls>",
    "webNavigation",
    "alarms",
    "storage"
  ]
}
